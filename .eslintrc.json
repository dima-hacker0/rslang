{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base/legacy",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-plusplus": 0,
        "lines-between-class-members": 0,
        "no-useless-concat": 0,
        "func-names": 0,
        "@typescript-eslint/no-var-requires": 0,
        "global-require": 0,
        "indent": 0,
        "no-useless-return": 0,
        "no-await-in-loop": 0,
        "no-use-before-define": 0,
        "max-len": 0,
        "no-param-reassign": 0,
        "no-loop-func": 0,
        "quote-props": 0,
        "no-underscore-dangle": 0,
        "no-prototype-builtins": 0,
        "no-lonely-if": 0,
        "no-restricted-globals": 0
    }
}