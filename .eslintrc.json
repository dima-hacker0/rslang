{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base/legacy",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-plusplus": 0,
        "lines-between-class-members": 0,
        "no-useless-concat": 0,
        "func-names": 0,
        "@typescript-eslint/no-var-requires": 0,
        "global-require": 0,
        "indent": 0
    }
}